<component prefix="collective.zws.datatypes">
  <sectiontype name="websocket-server"
               datatype=".WebSocketServerFactory"
               implements="ZServer.server">
     <key name="host" datatype="string">
       <description>
       Zope hostname (when dispatching requests for ZPublisher).
       </description>
     </key>
     <key name="port" datatype="integer" default="8081">
       <description>
       WebSocket server port.
       </description>
     </key>
  </sectiontype>
</component>
